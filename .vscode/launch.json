{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Test PandaService",
      "type": "Ruby",
      "request": "launch",
      "cwd": "${workspaceRoot}",
      "program": "${file}",
      "args": [
        "--workspace=../PandaService/",
        "--scheme=PandaService",
        "--command=1",
      ]
    },
    {
      "name": "Debug Local File",
      "type": "Ruby",
      "request": "launch",
      "program": "${workspaceRoot}/test/panthage_repo_helper_test.rb"
    },
    {
      "name": "Test panthage foo",
      "type": "Ruby",
      "request": "launch",
      "cwd": "${workspaceRoot}",
      "program": "${file}",
      "args": [
        "--workspace=../panthage_foo",
        "--scheme=panthage_foo",
        "--command=1",
      ]
    },
    {
      "name": "Test login slice",
      "type": "Ruby",
      "request": "launch",
      "cwd": "${workspaceRoot}",
      "program": "${file}",
      "args": [
        "--workspace=../login/",
        "--scheme=LoginSlice",
        "--command=1",
      ]
    },
    {
      "name": "Test Panda iOS",
      "type": "Ruby",
      "request": "launch",
      "cwd": "${workspaceRoot}",
      "program": "${file}",
      "args": [
        "install",
        "--workspace=../panda-ios/",
        "--scheme=Debug",
        "--verbose"
      ]
    }
  ]
}